<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.4" modifiedIn="OpenLP 2.9.4" modifiedDate="2022-06-13T16:16:56">
  <properties>
    <titles>
      <title>Jezus jest pośród nas</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
    <songbooks>
      <songbook name="Śpiewnik Wędrowiec" entry="279"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>/ Jezus jest pośród nas<br/>Obecny tu w Duchu swym,<br/>Wszystkich nas wzywa On:<br/>zbliżcie się /x2</lines>
    </verse>
    <verse name="v2">
      <lines>/ Znów dotknij serc nasz Panie<br/>Napełń nas swym Duchem<br/>Dotknij serc nasz Panie,<br/>napełń nas /x2</lines>
    </verse>
  </lyrics>
</song>
