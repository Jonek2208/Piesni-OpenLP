<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.4" modifiedIn="OpenLP 2.9.4" modifiedDate="2022-02-05T23:59:55">
  <properties>
    <titles>
      <title>Boże Tyś wierny jest</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Boże Tyś wierny jest<br/>O mnie troszczysz się,<br/>uwielbiam Cię<br/>Pokojem mym,<br/>Wybawcą jesteś mi<br/>Wywyższam Cię,<br/>Boże mój</lines>
    </verse>
  </lyrics>
</song>
