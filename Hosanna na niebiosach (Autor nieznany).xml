<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.4" modifiedIn="OpenLP 2.9.4" modifiedDate="2022-06-13T16:16:54">
  <properties>
    <titles>
      <title>Hosanna na niebiosach</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Hosanna, hosanna, hosanna na niebiosach!<br/>Sławić chcemy Cię wciąż z radością i czcią<br/>Wywyższony bądź Boże nasz!<br/>Hosanna niechaj ciągle brzmi!</lines>
    </verse>
  </lyrics>
</song>
