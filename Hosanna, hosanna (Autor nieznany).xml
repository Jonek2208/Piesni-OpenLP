<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.4" modifiedIn="OpenLP 2.9.4" modifiedDate="2022-02-05T23:59:57">
  <properties>
    <titles>
      <title>Hosanna, hosanna</title>
    </titles>
    <verseOrder>v1 c1 v2 c1 v3 c1 v4 c1</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>/ Hosanna, Hosanna,<br/>Królowi, królów cześć /x2</lines>
    </verse>
    <verse name="v2">
      <lines>/ Chwalmy Go najwyższy On<br/>i Godzien wszelkiej czci /x2</lines>
    </verse>
    <verse name="v3">
      <lines>/ Wielbimy, wielbimy,<br/>Wielbimy Panie Cię /x2</lines>
    </verse>
    <verse name="v4">
      <lines>/ Kochamy, kochamy,<br/>Kochamy Panie Cię /x2</lines>
    </verse>
    <verse name="c1">
      <lines>Chwalmy Pana śpiew niech brzmi,<br/>Zmartwychwstał Król i żyje dziś,</lines>
    </verse>
  </lyrics>
</song>
