  <properties>
    <titles>
      <title>Pan jest Pasterzem moim</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Pan jest Pasterzem moim<br/>Niczego mi nie braknie<br/><br/>Na zielonych niwach pasie mnie<br/>Nad wody spokojne prowadzi mnie</lines>
    </verse>
  </lyrics>
