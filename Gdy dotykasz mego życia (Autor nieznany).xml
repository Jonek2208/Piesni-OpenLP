<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.4" modifiedIn="OpenLP 2.9.4" modifiedDate="2022-06-13T16:16:53">
  <properties>
    <titles>
      <title>Gdy dotykasz mego życia</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
    <songbooks>
      <songbook name="Śpiewnik Wędrowiec" entry="224"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Gdy dotykasz mego życia się ręką Swą,<br/>To budzi w sercu moim pieśń:<br/>Panie, kocham Cię!</lines>
    </verse>
    <verse name="v2">
      <lines>Z głębi ducha ślę wyznania to miłe ci:<br/>Tyś jest mój Król, Tyś jest mój Bóg,<br/>Panie, kocham Cię!</lines>
    </verse>
  </lyrics>
</song>
