  <properties>
    <titles>
      <title>Niechaj miłość Twa</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Niechaj miłość Twa<br/>Jak potężna fala<br/>Spłynie tu<br/>Przez Łaski Twej zdrój<br/>Chryste dotknij nas</lines>
    </verse>
  </lyrics>
