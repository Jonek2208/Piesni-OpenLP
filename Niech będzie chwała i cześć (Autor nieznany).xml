<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.4" modifiedIn="OpenLP 2.9.4" modifiedDate="2022-06-13T17:56:33">
  <properties>
    <titles>
      <title>Niech będzie chwała i cześć</title>
    </titles>
    <verseOrder>v1 c1 v1 c1</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Niech będzie chwała i cześć<br/>I uwielbienie,<br/>Chwała i cześć Jezusowi.</lines>
    </verse>
    <verse name="c1">
      <lines>Chwała, niech będzie chwała,<br/>Tak, Jemu chwała i cześć!</lines>
    </verse>
  </lyrics>
</song>
