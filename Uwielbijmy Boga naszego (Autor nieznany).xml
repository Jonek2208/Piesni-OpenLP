<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.4" modifiedIn="OpenLP 2.9.4" modifiedDate="2022-06-13T16:17:05">
  <properties>
    <titles>
      <title>Uwielbijmy Boga naszego</title>
    </titles>
    <verseOrder>v1 v2 c1</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Uwielbijmy Boga naszego<br/>bo na wieki jego miłosierdzie.<br/>On wywyższy wszystkie swoje sługi.</lines>
    </verse>
    <verse name="v2">
      <lines>Uwielbijmy Boga naszego<br/>bo na wieki jego miłosierdzie.<br/>On poniży wszystkich swoich wrogów.</lines>
    </verse>
    <verse name="c1">
      <lines>Dzięki ci Jahwe, Hallelujah. /x2.</lines>
    </verse>
  </lyrics>
</song>
