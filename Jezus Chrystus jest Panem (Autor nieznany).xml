<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.4" modifiedIn="OpenLP 2.9.4" modifiedDate="2022-02-06T00:01:24">
  <properties>
    <titles>
      <title>Jezus Chrystus jest Panem</title>
    </titles>
    <verseOrder>v1 v2 v3 v4 v5</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
    <songbooks>
      <songbook name="Śpiewnik Wędrowiec" entry="284"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>/ Jezus Chrystus jest Panem Alleluja /x3<br/>Alleluja!</lines>
    </verse>
    <verse name="v2">
      <lines>/ Jezus Chrystus jest Królem Alleluja /x3<br/>Alleluja!</lines>
    </verse>
    <verse name="v3">
      <lines>/ Śpiewaj ziemio mu cała Alleluja /x3<br/>Alleluja!</lines>
    </verse>
    <verse name="v4">
      <lines>/ Więc śpiewamy Ci radośnie Alleluja /x3<br/>Alleluja!</lines>
    </verse>
    <verse name="v5">
      <lines>/ Alleluja, Alleluja, Alleluja /x3<br/>Alleluja!</lines>
    </verse>
  </lyrics>
</song>
