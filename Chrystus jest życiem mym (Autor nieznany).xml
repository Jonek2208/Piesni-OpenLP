<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.4" modifiedIn="OpenLP 2.9.4" modifiedDate="2022-06-13T16:16:51">
  <properties>
    <titles>
      <title>Chrystus jest życiem mym</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Chrystus jest życiem mym<br/>A śmierć zyskiem jest /x2<br/><br/>To mój Król, moja pieśń<br/>Życie me i radość ma<br/>Moja moc i mój miecz<br/>Pokój mój, tym jest mój Pan</lines>
    </verse>
  </lyrics>
</song>
