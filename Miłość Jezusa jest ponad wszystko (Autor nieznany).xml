  <properties>
    <titles>
      <title>Miłość Jezusa jest ponad wszystko</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Miłość Jezusa jest ponad wszystko /3x<br/>Alleluja<br/><br/>Bóg kocha mnie<br/>Takiego jakim jestem<br/>I cieszy się każdym<br/>Dobrym gestem<br/>Alleluja,<br/>Boża radość mnie rozpiera</lines>
    </verse>
  </lyrics>
