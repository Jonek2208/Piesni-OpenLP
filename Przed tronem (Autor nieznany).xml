<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.9.4" modifiedIn="OpenLP 2.9.4" modifiedDate="2022-06-13T16:17:02">
  <properties>
    <titles>
      <title>Przed tronem</title>
    </titles>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>/ PRZED TRONEM ŁASKI STAJĘ<br/>BY PRZYJĄĆ POMOC TWĄ<br/>PANIE POTRZEBUJĘ CIĘ<br/>SZUKAM TWARZY TWEJ /x2</lines>
    </verse>
    <verse name="v2">
      <lines>/ OJCZE DZIŚ PRZYCHODZĘ<br/>PRZEZ CHRYSTUSA KREW<br/>SKŁADAM CI W OFIERZE<br/>CAŁE ŻYCIE ME /x2</lines>
    </verse>
    <verse name="c1">
      <lines>/ BOŻE CHWAŁY, BOŻE MOCY, MAJESTATU /x2</lines>
    </verse>
  </lyrics>
</song>
