      <title>Tak mocno kocham Cię</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Tak mocno kocham Cię<br/>Tak mocno za Tobą tęsknię<br/>Tak mocno potrzebuję Ciebie<br/>Dziękuję, że jesteś, że jesteś</lines>
    </verse>
