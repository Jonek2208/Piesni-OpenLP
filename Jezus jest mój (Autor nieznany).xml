  <properties>
    <titles>
      <title>Jezus jest mój</title>
    </titles>
    <verseOrder>v1 v2 v3 v4</verseOrder>
    <authors>
      <author>Autor nieznany</author>
    </authors>
    <songbooks>
      <songbook name="Śpiewnik Wędrowiec" entry="239"/>
